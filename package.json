{
  "name": "style-this",
  "version": "0.1.0",
  "description": "CSS-in-JS library for the modern web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @style-this/core build",
    "build:vite": "pnpm --filter @style-this/vite build",
    "build:webpack": "pnpm --filter @style-this/webpack build",
    "dev": "pnpm -r dev",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@vitest/eslint-plugin": "1.4.1",
    "eslint": "9.39.1",
    "tsdown": "0.16.1",
    "typescript-eslint": "8.46.3",
    "vitest": "4.0.8"
  },
  "dependencies": {
    "@rollup/plugin-wasm": "6.2.2",
    "typescript": "5.9.3"
  }
}
