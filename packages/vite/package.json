{
  "name": "@style-this/vite",
  "version": "0.1.0",
  "description": "CSS-in-JS library for the modern web - vite plugin",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": "./dist/index.cjs",
    "./solid-js": {
      "import": "./dist/solid-js.cjs",
      "require": "./dist/solid-js.cjs",
      "types": "./dist/solid-js.d.cts",
      "default": "./dist/solid-js.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@style-this/core": "workspace:*",
    "vite": "7.2.2"
  },
  "peerDependencies": {
    "solid-js": "*"
  },
  "peerDependenciesMeta": {
    "solid-js": {
      "optional": true
    }
  }
}
